{
  "name": "vuejs-papercss",
  "version": "0.0.1",
  "description": "A VueJS version of the PaperCSS framework for all your Vue projects.",
  "main": "dist/papercss.js",
  "author": "Thomas Cazade <cazade.thomas@gmail.com>",
  "license": "MIT",
  "keywords": [
    "papercss",
    "vue",
    "vuejs",
    "components",
    "framework"
  ],
  "bugs": {
    "url": "https://github.com/papercss/vuejs-papercss/issues"
  },
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch"
  },
  "dependencies": {
    "papercss": "^1.6.0",
    "vue": "^2.5.18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "cross-env": "^5.2.0",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "vue-template-compiler": "^2.5.18"
  }
}
